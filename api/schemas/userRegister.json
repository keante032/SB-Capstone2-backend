{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/userRegister.json",
    "type": "object",
    "properties": {
        "username": {
            "type": "string",
            "minLength": 1,
            "maxLength": 32
        },
        "password": {
            "type": "string",
            "minLength": 5
        },
        "email": {
            "type": "string",
            "minLength": 6,
            "format": "email"
        }
    },
    "additionalProperties": false,
    "required": [
        "username",
        "password",
        "email"
    ]
}
